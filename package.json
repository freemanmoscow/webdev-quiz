{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "clean": "rm -f ./*.js; rm -f ./*.js.map; rm -f ./intermediates/*.js; rm -f ./intermediates/*.js.map",
    "tslint": "./node_modules/.bin/tslint ./*ts ./**/*.ts",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "go": "concurrent \"npm run tsc:w\" \"npm run serve\" ",
    "start": "concurrent \"npm run tsc:w\" \"npm run sass:watch\" \"npm run serve\" ",
    "typings": "typings install",
    "serve": "http-server . -p 8080",
    "postinstall": "npm run typings",
    "build": "npm run clean && tsc && cp ./app/app.js ./app/app.min.js && npm run sass",
    "build_prod": "npm run build && cp ./node_modules/pickadate/lib/picker.js ./node_modules/materialize-css/bin/ && ./node_modules/.bin/browserify -s app ./app/app.js > ./app/bundle.js && npm run minify && npm run sass && rm ./node_modules/materialize-css/bin/picker.js",
    "build_p": "npm run build && npm run minify",
    "minify": "./node_modules/.bin/uglifyjs app/bundle.js --screw-ie8 --compress --mangle --output app/app.min.js",
    "sass": "sass app/styles/styles.scss app/styles/styles.css --style compressed",
    "sass:watch": "sass --watch app:app --style compressed"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/platform-server": "^4.0.2",
    "@angular/router": "^4.0.2",
    "angular-in-memory-web-api": "~0.2.4",
    "angular-moment": "^1.0.1",
    "angular2-materialize": "^6.7.2",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.2",
    "jquery": "^3.2.1",
    "materialize-css": "^0.97.8",
    "moment": "^2.17.1",
    "pickadate": "^3.5.6",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.1.0",
    "systemjs": "0.19.41",
    "typescript": "^2.2.2",
    "uuid": "^3.0.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "browserify": "^14.3.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "jslint": "^0.10.3",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "minify": "^2.0.13",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^2.2.1",
    "typings": "^2.1.1",
    "typings-core": "^2.3.3",
    "uglify": "^0.1.5"
  },
  "repository": {}
}
